  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --exit-child -r tsconfig-paths/register src/server.ts",
    "build": "tsc -p tsconfig.json",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "format": "prettier \"src/**/*.{ts,tsx,json}\" --write",
    "test": "echo \"No tests yet\"",
    "seed:admin": "ts-node -r tsconfig-paths/register src/scripts/createAdmin.ts",
    "seed:questions": "ts-node -r tsconfig-paths/register src/scripts/importQuestions.ts",
    "update:computer-questions": "ts-node -r tsconfig-paths/register src/scripts/updateComputerQuestions.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@types/cookie-parser": "^1.4.10",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "rate-limiter-flexible": "^8.2.1",
    "telegraf": "^4.16.3",
    "uuid": "^13.0.0",
